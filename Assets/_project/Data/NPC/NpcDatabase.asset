  greetingsPhrases:
  - "\u0417\u0434\u043E\u0440\u043E\u0432\u043E, \u0445\u043E\u0437\u044F\u0438\u043D!
    \u0414\u0430\u0432\u043D\u043E \u043D\u0435 \u0432\u0438\u0434\u0435\u043B\u0438\u0441\u044C."
  - "\u042D\u0439, \u0434\u043E\u0431\u0440\u043E\u0433\u043E \u0434\u0435\u043D\u0451\u0447\u043A\u0430.
    \u0427\u0435\u0433\u043E \u043D\u043E\u0432\u0435\u043D\u044C\u043A\u043E\u0433\u043E?"
  - "\u0417\u0434\u0440\u0430\u0432 \u0431\u0443\u0434\u044C. \u042D\u0442\u043E
    \u0442\u044B \u0442\u043E\u0440\u0433\u0443\u0435\u0448\u044C \u0441\u0442\u0430\u0440\u044C\u0451\u043C?"
  - "\u041D\u0443 \u043F\u0440\u0438\u0432\u0435\u0442. \u0421\u043B\u044B\u0448\u0430\u043B,
    \u0443 \u0442\u0435\u0431\u044F \u0442\u0443\u0442 \u0438\u043D\u0442\u0435\u0440\u0435\u0441\u043D\u044B\u0435
    \u0432\u0435\u0449\u0438\u0447\u043A\u0438 \u0431\u044B\u0432\u0430\u044E\u0442."
  goodbuyPhrases:
  - "\u042D\u0445, \u043D\u0438\u0447\u0435\u0433\u043E \u043D\u0435 \u043F\u0440\u0438\u0433\u043B\u044F\u043D\u0443\u043B\u043E\u0441\u044C.
    \u0411\u044B\u0432\u0430\u0439."
  - "\u041D\u0443\u2026 \u0434\u043E\u0440\u043E\u0433\u043E \u0443 \u0442\u0435\u0431\u044F,
    \u0445\u043E\u0437\u044F\u0438\u043D. \u0417\u0430\u0433\u043B\u044F\u043D\u0443
    \u043A\u0430\u043A \u0440\u0430\u0437\u0431\u043E\u0433\u0430\u0442\u0435\u044E."
  - "\u041B\u0430\u0434\u043D\u043E, \u043F\u043E\u0439\u0434\u0443 \u044F. \u041C\u043E\u0436\u0435\u0442,
    \u0432 \u0434\u0440\u0443\u0433\u043E\u0439 \u0440\u0430\u0437."
  - "\u041D\u0435 \u0441\u0443\u0434\u044C\u0431\u0430 \u0441\u0435\u0433\u043E\u0434\u043D\u044F.
    \u0421\u0447\u0430\u0441\u0442\u043B\u0438\u0432\u043E."
  learnAssortimentPhrases:
  - "\u0414\u0430\u0439-\u043A\u0430 \u043F\u043E\u0433\u043B\u044F\u0436\u0443,
    \u0447\u0442\u043E \u0443 \u0442\u0435\u0431\u044F \u0442\u0443\u0442 \u043D\u0430
    \u043F\u0440\u0438\u043B\u0430\u0432\u043A\u0435."
  - "\u0425\u043E\u0447\u0443 \u043F\u0440\u0438\u0446\u0435\u043D\u0438\u0442\u044C\u0441\u044F\u2026
    \u0432\u0434\u0440\u0443\u0433 \u0447\u0435\u0433\u043E \u043D\u0443\u0436\u043D\u043E\u0435
    \u043D\u0430\u0439\u0434\u0451\u0442\u0441\u044F."
  - "\u041D\u0435 \u0441\u043F\u0435\u0448\u0438, \u044F \u043F\u0440\u043E\u0441\u0442\u043E
    \u043F\u043E\u0441\u043C\u043E\u0442\u0440\u044E, \u043B\u0430\u0434\u043D\u043E?"
  - "\u041D\u0443-\u043A\u0430, \u0447\u0442\u043E \u044D\u0442\u043E \u0443 \u0442\u0435\u0431\u044F
    \u0442\u0443\u0442\u2026 \u043B\u044E\u0431\u043E\u043F\u044B\u0442\u043D\u043E."
  interestPhrases:
  - "\u0421\u043B\u0443\u0448\u0430\u0439, \u0430 \u0432\u043E\u0442 \u044D\u0442\u043E
    \u0441\u043A\u043E\u043B\u044C\u043A\u043E \u0441\u0442\u043E\u0438\u0442?"
  - "\u0412\u043E\u0442 \u044D\u0442\u0430 \u0448\u0442\u0443\u043A\u0430\u2026 \u043F\u043E\u0447\u0451\u043C
    \u043E\u0442\u0434\u0430\u0448\u044C?"
  - "\u041D\u0443-\u043A\u0430, \u0430 \u044D\u0442\u0430 \u0432\u0435\u0449\u0438\u0446\u0430
    \u0434\u043E\u0440\u043E\u0433\u0430\u044F?"
  - "\u0421\u043A\u0430\u0436\u0438 \u0446\u0435\u043D\u0443, \u043C\u043E\u0436\u0435\u0442,
    \u0441\u0442\u043E\u0440\u0433\u0443\u0435\u043C\u0441\u044F."
  thanksPhrases:
  - "\u0412\u043E\u0442 \u044D\u0442\u043E \u043F\u043E-\u043D\u0430\u0448\u0435\u043C\u0443.
    \u0421\u043F\u0430\u0441\u0438\u0431\u043E!"
  - "\u041D\u0443, \u043F\u0440\u0438\u044F\u0442\u043D\u043E \u0438\u043C\u0435\u0442\u044C
    \u0441 \u0442\u043E\u0431\u043E\u0439 \u0434\u0435\u043B\u043E."
  - "\u0425\u043E\u0440\u043E\u0448\u0430\u044F \u0441\u0434\u0435\u043B\u043A\u0430,
    \u0445\u043E\u0437\u044F\u0438\u043D. \u0415\u0449\u0451 \u0437\u0430\u0433\u043B\u044F\u043D\u0443."
  - "\u041D\u0443, \u0431\u044B\u0432\u0430\u0439. \u0414\u0435\u043D\u044C \u0443\u0434\u0430\u043B\u0441\u044F!"
  bargainPhrases:
  - "\u042D\u0439, \u043D\u0443 \u0442\u044B \u0437\u0430\u0433\u043D\u0443\u043B!
    \u041C\u043E\u0436\u0435\u0442, \u0441\u043A\u0438\u043D\u0435\u043C \u043D\u0435\u043C\u043D\u043E\u0433\u043E?"
  - "\u0414\u043E\u0440\u043E\u0433\u043E \u0431\u0435\u0440\u0451\u0448\u044C. \u0414\u0430\u0432\u0430\u0439
    \u043F\u043E-\u0434\u0440\u0443\u0436\u0435\u0441\u043A\u0438."
  - "\u042D\u0439, \u043D\u0443 \u0447\u0451, \u043D\u0435\u0443\u0436\u0442\u043E
    \u0441\u043A\u0438\u0434\u043A\u0438 \u043D\u0435 \u0431\u0443\u0434\u0435\u0442?"
